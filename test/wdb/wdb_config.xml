<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE wdb_fimex_config>
<wdb_fimex_config xmlns:xi="http://www.w3.org/2001/XInclude">
<global_attributes>
    <attribute name="Conventions" value="CF-1.0" type="string" />
    <attribute name="institution" value="Norwegian Meteorological Institute" type="string" />
    <attribute name="product_name" value="WdbCDMReader" type="string" />
    <attribute name="project_name" value="WdbCDMReader" type="string" />
    <attribute name="title" value="Wdb data extraction via Fimex" type="string" />
    <attribute name="topiccategory" value="ClimatologyMeteorologyAtmosphere" type="string" />
    <attribute name="contact" value="fimex@lists.met.no" type="string" />
    <attribute name="distribution_statement" value="Free" type="string" />
</global_attributes>

<wdb_parameters>

    <value_parameter wdbname="air temperature">
    	<attribute name="units" value="K" type="string" />
        <attribute name="standard_name" value="air_temperature" type="string" />
    </value_parameter>

	<value_parameter wdbname="mean sea level pressure" cfname="mslp">
		<attribute name="units" value="Pa" type="string" />
		<attribute name="standard_name" value="air_pressure_at_sea_level" type="string" />
	</value_parameter>

    <value_parameter wdbname="x wind">
		<attribute name="units" value="m s-1" type="string" />    
        <attribute name="standard_name" value="eastward_wind" type="string" />
    </value_parameter>

    <value_parameter wdbname="y wind">
    	<attribute name="units" value="m s-1" type="string" />
        <attribute name="standard_name" value="northward_wind" type="string" />
    </value_parameter>

    <value_parameter wdbname="high cloud cover">
    	<attribute name="units" value="1" type="string" />
        <attribute name="standard_name" value="cloud_area_fraction_in_atmosphere_layer" type="string" />
        <attribute name="long_name" value="stuff" type="string" />
        <attribute name="DefaultValue_" value="0" type="float" />
    </value_parameter>

</wdb_parameters>
</wdb_fimex_config>
