<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE wdb_fimex_config>
<wdb_fimex_config xmlns:xi="http://www.w3.org/2001/XInclude">
<global_attributes>
    <attribute name="Conventions" value="CF-1.0" type="string" />
    <attribute name="institution" value="Norwegian Meteorological Institute" type="string" />
    <attribute name="product_name" value="WdbCDMReader" type="string" />
    <attribute name="project_name" value="WdbCDMReader" type="string" />
    <attribute name="title" value="Wdb data extraction via Fimex" type="string" />
    <attribute name="topiccategory" value="ClimatologyMeteorologyAtmosphere" type="string" />
    <attribute name="contact" value="fimex@lists.met.no" type="string" />
    <attribute name="distribution_statement" value="Free" type="string" />
</global_attributes>

<wdb_parameters>

    <wdb_connection id="0" name="local_wdb" type="string">
        <attribute name="dbhost" value="localhost" type="string" />
        <attribute name="dbport" value="5432" type="short" />
        <attribute name="dbname" value="wdb" type="string" />
        <attribute name="dbuser" value="vegardb" type="string" />
        <attribute name="wciuser" value="vegardb" type="string" />
    </wdb_connection>

    <data_providers>
		<provider id="0" name="logic_name1" value="met.no eceps modification"></provider>
    </data_providers>

    <grid_places>
        <place id="0" name="logic_name2" value="norway 025"></place>
    </grid_places>

    <level_parameter id="0" name="atmosphere sigma coordinate">
        <attribute name="standard_name" value="sigma" type="string" />
    </level_parameter>

    <level_parameter id="1" name="height above ground">
        <attribute name="standard_name" value="height" type="string" />
    </level_parameter>


    <value_parameter id="air temperature" name="air temperature">
        <attribute name="standard_name" value="air_temperature" type="string" />
        <attribute name="units" value="K" type="string" />
        <attribute name="_FillValue" value="0.0" type="float" />
    </value_parameter>

    <value_parameter id="cloud area fraction" name="cloud area fraction">
        <attribute name="standard_name" value="cloud_area_fraction" type="string" />
        <attribute name="_FillValue" value="0.0" type="float" />
    </value_parameter>

</wdb_parameters>
</wdb_fimex_config>
